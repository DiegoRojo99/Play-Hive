datasource db {
  provider    = "postgresql"
  url         = env("DATABASE_URL")
  directUrl   = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Game {
  id            Int    @id @default(autoincrement())
  appid         String @unique
  name          String
  genres        String
  description   String?
}